<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.game">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.LeaderboardActivity"></activity>
        <activity android:name=".Activities.EditNameActivity" />
        <activity android:name=".Activities.ShopActivity" />
        <activity android:name=".Activities.GameActivity" />
        <activity android:name=".Activities.LogInActivity" />
        <activity android:name=".Activities.SignUpActivity" />
        <activity android:name=".Activities.GameResultsActivity" />
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.SettingsActivity" />
        <activity android:name=".Activities.PracticeActivity" />
        <activity android:name=".Activities.HowToPlayActivity" />
    </application>

</manifest>